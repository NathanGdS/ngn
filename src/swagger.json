{
    "openapi": "3.0.0",
    "info": {
        "title": "RentalX Documentation",
        "description": "This is an API Rent",
        "version": "1.0.0",
        "contact": {
            "email":"nathangarsantos@gmail.com"
        }
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/automovel/tipo": {
            "get": {
                "tags": ["Automovel"],
                "summary": "List all Tipo Automovel",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            },
                                            "created_at": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": ["Automovel"],
                "summary": "Create a new Tipo Automovel",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "description": {
                                        "type":"string"
                                    }
                                }
                            },
                            "example": {
                                "description": "SUV"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            },
                                            "created_at": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    
                    "400": {
                        "description": "Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "error": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/automovel/tipo/{tipo_id}": {
            "patch": {
                "tags": ["Automovel"],
                "summary": "Update Description of Tipo Automovel",
                "parameters": [
                    {
                        "name": "tipo_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type":"string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "description": {
                                        "type":"string"
                                    }
                                }
                            },
                            "example": {
                                "description": "SUV V2"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Updated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            },
                                            "created_at": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": ["Automovel"],
                "summary": "Delete an Tipo Automovel",
                "parameters": [
                    {
                        "name": "tipo_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type":"string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        }
    }
}